{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build - Metarwiz-Console (Debug)",
            "program": "${workspaceFolder}${pathSeparator}metarwiz-console${pathSeparator}bin${pathSeparator}Debug${pathSeparator}net5.0${pathSeparator}metarwiz-console.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}